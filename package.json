{
  "name": "dojo-react-ourly-setting",
  "author": "akky--xxxx <114.s.aki@gmail.com>",
  "scripts": {
    "agreed": "ts-node --project ./tsconfig.server.json --files ./spec/api/index.ts",
    "agreed:doc": "agreed-ui --path spec/api/agreed.ts --port 4002",
    "check-all": "npm-run-all fix -p spell-check lint:css type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "run-p start:dev agreed",
    "fix": "run-p fix:* -c",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:* -c",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "start:dev": "next dev",
    "storybook": "start-storybook -s ./public -p 6006",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "daisyui": "^2.43.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@agreed/cli": "^5.2.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/jest": "^0.0.9",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/testing-react": "^1.2.3",
    "@swc/core": "^1.2.150",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/styled-components": "^5.1.24",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cspell": "^5.18.5",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-unicorn": "^41.0.0",
    "fixpack": "^4.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "storybook-addon-next-router": "^3.1.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.5.5"
  },
  "version": "1.0.0",
  "license": "MIT"
}
